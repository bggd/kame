CFLAGS=
LIBFLAG=
LUA_LIB=
LUA_INCDIR=
LUA_LIBDIR=
INST_PREFIX=

all:
	cmake -S . -B build -DCMAKE_C_FLAGS="$(CFLAGS)" -DCMAKE_EXE_LINKER_FLAGS="$(LIBFLAG)" -DKAME_LUA_INCDIR=$(LUA_INCDIR) -DKAME_LUALIB=$(LUA_LIBDIR)\$(LUALIB) -DKAME_BINDING_LUA=OK -DCMAKE_INSTALL_PREFIX=$(INST_PREFIX)

install:
	cmake --build build --config Release --target install --parallel
